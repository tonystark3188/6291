+static int assign_pair(struct elem_to_channel e2c_vec[MAX_ELEM_ID],                                                                                      
+                       uint8_t (*layout_map)[3], int offset, uint64_t left,                                                                              
+    uint64_t right, int pos)                                                                                                                             
+{                                                                                                                                                        
