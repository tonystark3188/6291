+static void make_filters_from_proto(int (*filter)[8][2], const int *proto, int bands)                     
+{                                   
+                                    
+    const int *sinptr, *cosptr;     
+    int s, c, sinhalf, coshalf;     
+    int q, n;                       
+    long long accu;                 
+                                    
+    if (bands == 4) {               
+        sinptr = sintbl_4;          
+        cosptr = costbl_4;          
