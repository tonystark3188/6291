+static int array_min_int16(const int16_t *array, int nel)                                                                                                   
+{                                                                                                                                                           
+    int i, min = array[0];                                                                                                                                  
+    for (i = 1; i < nel; i++)                                                                                                                               
+        min = FFMIN(array[i], min);                                                                                                                         
+    return min;                                                                                                                                             
+}                                                                                                                                                           
