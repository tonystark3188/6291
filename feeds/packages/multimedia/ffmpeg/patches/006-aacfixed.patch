+        coup->id_select[c] = get_bits(gb, 4);                                                                                                            
+        if (coup->type[c] == TYPE_CPE) {                                                                                                                 
+            coup->ch_select[c] = get_bits(gb, 2);                                                                                                        
+            if (coup->ch_select[c] == 3)                                                                                                                 
+                num_gain++;                                                                                                                              
+        } else                                                                                                                                           
+            coup->ch_select[c] = 2;                                                                                                                      
