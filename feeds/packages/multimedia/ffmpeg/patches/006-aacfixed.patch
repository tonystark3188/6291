+        ac->oc[1].m4ac.channels = avctx->channels;                                                                                                       
+        ac->oc[1].m4ac.sbr = -1;                                                                                                                         
+        ac->oc[1].m4ac.ps = -1;                                                                                                                          
+                                                                                                                                                         
+        for (i = 0; i < FF_ARRAY_ELEMS(ff_mpeg4audio_channels); i++)                                                                                     
+            if (ff_mpeg4audio_channels[i] == avctx->channels)                                                                                            
+                break;                                                                                                                                   
+        if (i == FF_ARRAY_ELEMS(ff_mpeg4audio_channels)) {                                                                                               
+            i = 0;                                                                                                                                       
